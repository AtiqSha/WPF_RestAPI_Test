<Window x:Class="AEM_Test.MainMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AEM_Test"
        mc:Ignorable="d"
        Title="MainMenu" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.504,1.5" StartPoint="0.504,0.03">
                <GradientStop Color="#1261A0" Offset="0"/>
                <GradientStop Color="#99fadc" Offset="0.567"/>
            </LinearGradientBrush>
        </Grid.Background>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button x:Name ="GPWActual"  Content="Rerun GetPlatformWellActual()" Width="250" Height="30" Margin="0,0,0,0" FontFamily="Adobe Heiti Std R" FontSize="14" Click="GPWActual_Click" />
            <Button x:Name ="GPWDummy" Content="Run GetPlatformWellDummy()" Width="250" Height="30" Margin="0,30,0,0" FontFamily="Adobe Heiti Std R" FontSize="14" Click="GPWDummy_Click" />
        </StackPanel>
    </Grid>
</Window>
